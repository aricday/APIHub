{
    "name": "layer7-apihub",
    "version": "1.0.0",
    "main": "lib/index.js",
    "module": "esm/index.js",
    "sideEffects": false,
    "scripts": {
        "build": "yarn run build-cjs && yarn run build-esm",
        "build-cjs": "rimraf ./lib && tsc",
        "build-esm": "rimraf ./esm && tsc --outDir esm --module es2015",
        "dev": "tsc --incremental --watch --outDir esm --module es2015",
        "storybook": "start-storybook -p 9009 -s public",
        "build-storybook": "build-storybook -c .storybook -o build",
        "test": "kcd-scripts test"
    },
    "dependencies": {
        "@blackbox-vision/ra-language-spanish": "~3.2.0",
        "@material-ui/core": "~4.9.3",
        "@material-ui/lab": "~4.0.0-alpha.45",
        "@rehooks/local-storage": "~2.3.0",
        "jsencrypt": "~3.0.0-rc.1",
        "ra-i18n-polyglot": "~3.4.1",
        "ra-language-english": "~3.4.1",
        "ra-language-french": "~3.4.1",
        "ra-language-italian": "~3.0.0",
        "react-admin": "~3.4.1",
        "react-markdown-editor-lite": "~1.0.2",
        "remark-parse": "~7.0.2",
        "remark-react": "~6.0.0",
        "remove-markdown": "0.3.0",
        "slugify": "~1.4.0",
        "swagger-ui-react": "~3.25.0",
        "unified": "~8.4.2"
    },
    "peerDependencies": {
        "react": "~16.13.0",
        "react-dom": "~16.13.0"
    },
    "devDependencies": {
        "@storybook/addon-actions": "~5.3.18",
        "@storybook/addon-knobs": "~5.3.18",
        "@storybook/addon-links": "~5.3.18",
        "@storybook/addons": "~5.3.18",
        "@storybook/preset-create-react-app": "~1.5.2",
        "@storybook/react": "~5.3.18",
        "@testing-library/jest-dom": "~4.2.4",
        "@testing-library/react": "~9.3.2",
        "@testing-library/user-event": "~7.1.2",
        "fetch-mock": "~8.3.2",
        "identity-obj-proxy": "~3.0.0",
        "jest-localstorage-mock": "~2.4.0",
        "kcd-scripts": "~5.0.0",
        "layer7-apihub-mock": "1.0.0",
        "react": "~16.13.0",
        "react-dom": "~16.13.0",
        "react-scripts": "~3.2.0",
        "rimraf": "~3.0.2",
        "typescript": "~3.7.5"
    },
    "files": [
        "*.md",
        "lib",
        "esm",
        "src"
    ]
}
